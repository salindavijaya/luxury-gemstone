{
  "name": "luxury-gemstone-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Luxury gemstone e-commerce platform built with Vue 3 and Vite",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.19",
    "@heroicons/vue": "^2.1.3",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.8",
    "pinia": "^2.1.7",
    "swiper": "^11.1.0",
    "vee-validate": "^4.12.6",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue3-toastify": "^0.2.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vite-plugin-pwa": "^1.0.3",
    "vue-tsc": "^2.0.11"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
